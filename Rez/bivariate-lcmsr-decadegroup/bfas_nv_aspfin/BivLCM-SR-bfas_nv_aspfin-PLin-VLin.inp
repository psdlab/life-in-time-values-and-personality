USEVARIABLES =
abfas_nv
bbfas_nv
cbfas_nv
dbfas_nv
aaspfin
baspfin
caspfin
daspfin
a1
a2
a3
a4
aagegrp
;

missing = all (-9999);

tscores = a1 a2 a3 a4;

grouping = aagegrp (0=d2, 1=d3, 2=d4, 3=d5);

DEFINE:

aagegrp = aage;

cut aagegrp (29 39 49);

a1 = aage + 0 - 36;
a2 = aage + 1 - 36;
a3 = aage + 2 - 36;
a4 = aage + 3 - 36;

ANALYSIS:
TYPE=RANDOM;
PROCESSORS = 6; 
model=nocovariances; ! supresses default estimation of certain covs in model;

MODEL:

i_bfas_nv s_bfas_nv | abfas_nv bbfas_nv cbfas_nv dbfas_nv AT a1 a2 a3 a4;
i_aspfin s_aspfin | aaspfin baspfin caspfin daspfin AT a1 a2 a3 a4;

! freeing factor covs because of "nocovariances"
! But ensure they are constrained across groups
s_bfas_nv WITH i_bfas_nv (p)
s_aspfin (q)
i_aspfin (r); 
i_bfas_nv WITH s_aspfin (s)
i_aspfin (t);
s_aspfin WITH i_aspfin (u);

!constrain latent growth means across groups
[i_bfas_nv] (v); 
[s_bfas_nv] (w);
[i_aspfin] (x); 
[s_aspfin] (y);

!constrain latent growth variances across groups
i_bfas_nv (l);
s_bfas_nv (m);
i_aspfin (n);
s_aspfin (o);

p1 BY abfas_nv@1; ! Residual "factors";
p2 BY bbfas_nv@1;
p3 BY cbfas_nv@1;
p4 BY dbfas_nv@1;

v1 BY aaspfin@1; ! Residual "factors";
v2 BY baspfin@1;
v3 BY caspfin@1;
v4 BY daspfin@1;

!residual variances on observed/manifest vars set to 0
abfas_nv@0;
bbfas_nv@0;
cbfas_nv@0;
dbfas_nv@0;

!residual variances on observed/manifest vars set to 0
aaspfin@0;
baspfin@0;
caspfin@0;
daspfin@0;

! means of observed/manifest vars set to 0
[abfas_nv@0];
[bbfas_nv@0];
[cbfas_nv@0];
[dbfas_nv@0];

! means of observed/manifest vars set to 0
[aaspfin@0];
[baspfin@0];
[caspfin@0];
[daspfin@0];

[p1@0]; ! means of residual "factors" set to 0
[p2@0];
[p3@0];
[p4@0];

[v1@0]; ! means of residual "factors" set to 0
[v2@0];
[v3@0];
[v4@0];

p4 on p3 (a); ! Autoregressive paths constrained to equality
p3 on p2 (a);
p2 on p1 (a);

v4 on v3 (b); ! Autoregressive paths constrained to equality
v3 on v2 (b);
v2 on v1 (b);

p4 on v3 (c); ! Cross-lagged paths constrained to equality
p3 on v2 (c);
p2 on v1 (c);

v4 on p3 (d); ! Cross-lagged paths constrained to equality
v3 on p2 (d);
v2 on p1 (d);

p1 with v1; !(this one is not like the others)

p2 with v2 (e); ! Within-wave correlations constrained to equality
p3 with v3 (e);
p4 with v4 (e);

p2 p3 p4 (f); ! estimate variances of residual "factors"
v2 v3 v4 (g); ! estimate variances of residual "factors"

MODEL d2:

p4 on p3 (a2); ! Autoregressive paths constrained to equality
p3 on p2 (a2);
p2 on p1 (a2);

v4 on v3 (b2); ! Autoregressive paths constrained to equality
v3 on v2 (b2);
v2 on v1 (b2);

p4 on v3 (c2); ! Cross-lagged paths constrained to equality
p3 on v2 (c2);
p2 on v1 (c2);

v4 on p3 (d2); ! Cross-lagged paths constrained to equality
v3 on p2 (d2);
v2 on p1 (d2);

p1 with v1; !(this one is not like the others)

p2 with v2 (e2); ! Within-wave correlations constrained to equality
p3 with v3 (e2);
p4 with v4 (e2);

p2 p3 p4 (f2); ! estimate variances of residual "factors"
v2 v3 v4 (g2); ! estimate variances of residual "factors"

MODEL d3:

p4 on p3 (a3); ! Autoregressive paths constrained to equality
p3 on p2 (a3);
p2 on p1 (a3);

v4 on v3 (b3); ! Autoregressive paths constrained to equality
v3 on v2 (b3);
v2 on v1 (b3);

p4 on v3 (c3); ! Cross-lagged paths constrained to equality
p3 on v2 (c3);
p2 on v1 (c3);

v4 on p3 (d3); ! Cross-lagged paths constrained to equality
v3 on p2 (d3);
v2 on p1 (d3);

p1 with v1; !(this one is not like the others)

p2 with v2 (e3); ! Within-wave correlations constrained to equality
p3 with v3 (e3);
p4 with v4 (e3);

p2 p3 p4 (f3); ! estimate variances of residual "factors"
v2 v3 v4 (g3); ! estimate variances of residual "factors"

MODEL d4:

p4 on p3 (a4); ! Autoregressive paths constrained to equality
p3 on p2 (a4);
p2 on p1 (a4);

v4 on v3 (b4); ! Autoregressive paths constrained to equality
v3 on v2 (b4);
v2 on v1 (b4);

p4 on v3 (c4); ! Cross-lagged paths constrained to equality
p3 on v2 (c4);
p2 on v1 (c4);

v4 on p3 (d4); ! Cross-lagged paths constrained to equality
v3 on p2 (d4);
v2 on p1 (d4);

p1 with v1; !(this one is not like the others)

p2 with v2 (e4); ! Within-wave correlations constrained to equality
p3 with v3 (e4);
p4 with v4 (e4);

p2 p3 p4 (f4); ! estimate variances of residual "factors"
v2 v3 v4 (g4); ! estimate variances of residual "factors"

MODEL d5:

p4 on p3 (a5); ! Autoregressive paths constrained to equality
p3 on p2 (a5);
p2 on p1 (a5);

v4 on v3 (b5); ! Autoregressive paths constrained to equality
v3 on v2 (b5);
v2 on v1 (b5);

p4 on v3 (c5); ! Cross-lagged paths constrained to equality
p3 on v2 (c5);
p2 on v1 (c5);

v4 on p3 (d5); ! Cross-lagged paths constrained to equality
v3 on p2 (d5);
v2 on p1 (d5);

p1 with v1; !(this one is not like the others)

p2 with v2 (e5); ! Within-wave correlations constrained to equality
p3 with v3 (e5);
p4 with v4 (e5);

p2 p3 p4 (f5); ! estimate variances of residual "factors"
v2 v3 v4 (g5); ! estimate variances of residual "factors"

OUTPUT: SAMPSTAT PATTERNS;
